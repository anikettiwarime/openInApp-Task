{
  "name": "basicnodeapp",
  "version": "1.0.0",
  "description": "Nodejs app structure with auth",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.6"
  },
  "scripts": {
    "start": "node --env-file=.env src/index.js",
    "dev": "nodemon --env-file=.env src/index.js",
    "dev:prettier": "nodemon --env-file=.env -x \"npm run prettier && node\" src/index.js",
    "prettier": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "node",
    "mongodb",
    "express",
    "backend",
    "template"
  ],
  "author": "aniket tiwari",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.0",
    "mongoose-aggregate-paginate-v2": "^1.0.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4"
  }
}
